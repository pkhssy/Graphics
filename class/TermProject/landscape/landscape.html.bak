<!DOCTYPE html>
<html>
<head>
<script id="vertex-shader" type="x-shader/x-vertex">
attribute vec4 vPosition;
attribute vec4 vColor;
varying vec4 fColor;
void main()
{
	fColor=vColor;
	gl_Position = vPosition;
}
</script>

<script id="fragment-shader" type="x-shader/x-fragment">
precision mediump float;
varying vec4 fColor;
void main()
{
	gl_FragColor = fColor;
}
</script>


<script id="Lvertex-shader" type="x-shader/x-vertex">
attribute vec4 vPosition;
uniform vec4 vOffset;
void main()
{
	gl_Position = vPosition + vOffset;

}
</script>

<script id="Lvertex_ground-shader" type="x-shader/x-vertex">
attribute vec4 vPosition_ground;
attribute vec4 vColor_ground;
varying vec4 fColor_ground;
void main()
{
	fColor_ground = vColor_ground;
	gl_Position = vPosition_ground;
}
</script>

<script id="Lfragment-shader" type="x-shader/x-fragment">
precision mediump float;
uniform vec4 color;
void main()
{
	gl_FragColor = color;
}
</script>

<script id="Lfragment_ground-shader" type="x-shader/x-fragment">
precision mediump float;
varying vec4 fColor_ground;
void main()
{
	gl_FragColor = fColor_ground;
}
</script>

<script id="Lvertex_sky-shader" type="x-shader/x-vertex">
attribute vec4 vPosition_sky;
attribute vec4 vColor_sky;
varying vec4 fColor_sky;
void main()
{
	fColor_sky = vColor_sky;
	gl_Position = vPosition_sky;
}
</script>


<script id="Lfragment_sky-shader" type="x-shader/x-fragment">
precision mediump float;
varying vec4 fColor_sky;
void main()
{
	gl_FragColor = fColor_sky;
}
</script>

<script type="text/javascript" src="../Common/webgl-utils.js"></script>
<script type="text/javascript" src="../Common/initShaders.js"></script>
<script type="text/javascript" src="../Common/MV.js"></script>
<script type="text/javascript" src="landscape.js"></script>
</head>
<body>
<canvas id="gl-canvas" width="512" height="512">
Oops ... your browser doesn't support the HTML5 canvas element
</canvas>
</body>
</html>